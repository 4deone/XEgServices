<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.EgServices" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_700</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="backgroundColor">@color/backgroundColor_jour</item>
        <item name="textColor">@color/textColor_jour</item>
        <item name="titreTextColor">@color/titreTextColor_jour</item>
        <item name="android:navigationBarColor">@color/purple_700</item>
        <item name="backgroundEdtv">@color/backgroundEdtv_jour</item>
        <item name="backgroundEdtvBorder">@color/backgroundEdtvBorder_jour</item>
    </style>
</resources>